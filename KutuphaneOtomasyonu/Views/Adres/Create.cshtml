@model KutuphaneOtomasyonu.Models.Adres

@{
    ViewData["Title"] = "Create";
}

 

<h2>Adres Oluştur</h2>
<hr />
 
    <div class="col-md-10">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Il" class="control-label"></label>
                <input asp-for="Il" class="form-control" />
                <span asp-validation-for="Il" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Ilce" class="control-label"></label>
                <input asp-for="Ilce" class="form-control" />
                <span asp-validation-for="Ilce" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AdresTarifi" class="control-label"></label>
                <input asp-for="AdresTarifi" class="form-control" />
                <span asp-validation-for="AdresTarifi" class="text-danger"></span>
            </div> 
            <div class="form-group">
                <input type="submit" value="Oluştur" class="btn btn-primary" />
            </div>
        </form>
    </div>
 

<div>
    <a asp-action="Index">Listeye Geri Dön</a>
</div>

