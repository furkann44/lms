@model KutuphaneOtomasyonu.Models.Adres

@{
    ViewData["Title"] = "Edit";
}

 

<h2>Adres Düzenle</h2>
<hr />
 
    <div class="form-horizontal">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                    <label asp-for="Il" class="col-md-2 control-label"></label>
                <div class="col-md-10">
                    <input asp-for="Il" class="form-control" />
                    <span asp-validation-for="Il" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group">
                    <label asp-for="Ilce" class="col-md-2 control-label"></label>
                <div class="col-md-10">
                    <input asp-for="Ilce" class="form-control" />
                    <span asp-validation-for="Ilce" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group">
                    <label asp-for="AdresTarifi" class="col-md-2 control-label"></label>
                <div class="col-md-10">
                    <input asp-for="AdresTarifi" class="form-control" />
                    <span asp-validation-for="AdresTarifi" class="text-danger"></span>
                </div>
            </div>
            <div class="form-group">
                <input type="submit" value="Düzenle" class="btn btn-primary" />
            </div>
        </form>
    </div>
 

<div>
    <a asp-action="Index">Listeye Geri Dön</a>
</div>

